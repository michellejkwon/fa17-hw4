<h1>Cats</h1>
<% @cats.each do |cat| %>
  <%= cat.name %>
  <br/>
<% end %>

<h1>Todos</h1>
<% @todos.each do |todo| %>
  <%= todo.tasks %><br/>
  <%= todo.finished %>
  <br/>
<% end %>

<h1>Users</h1>
<% @users.each do |user| %>
  <%= user.name %>
  <%= user.email %>
  <%= user.age %>
  <br/>
<% end %>
